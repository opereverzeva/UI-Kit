-top: 7px;\n    width:100%;\n    height:68px;\n    display:flex;\n    flex-direction:column;\n    justify-content:center;\n    align-items:center;\n    padding: 0px;\n}\n.calendar__current-month{\n    margin-top: 16px;\n    display:flex;\n    justify-content:space-between;\n    align-items:center;\n    height: 24px;\n    width: 100%;\n}\n.calendar__button-pre, .calendar__button-next{\n    width:24px;\n    height:24px;\n    /* margin-top:4px; */\n    background: #FFFFFF;\n    box-shadow: 0px 7px 20px rgba(0, 0, 0, 0.07);\n    border-radius: 6px;\n    display:flex;\n    justify-content: center;\n    align-items: center;\n    cursor: pointer;\n}\n.calendar__button-pre{\n    color:#ECEBED;\n    margin-left:16px;\n}\n.calendar__month-name{\n    flex-grow:1;\n    font-family: -apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif;\n    line-height: 20px;\n    font-size: 15px;\n    text-align: center;\n    text-justify: auto;\n    color: #1A051D;\n}\n.calendar__button-next{\n    margin-right:16px;\n}\n.calendar__title-days{\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin: 10px 16px 9px 16px;\n    width: 288px;\n    font-family: -apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif;\n    line-height: 16px;\n    font-size: 12px;\n    color: #D0C9D6;\n    padding: 0px;\n\n}\n.calendar__line{\n    /* bottom: 0; */\n    width: 288px;\n    margin: 0px;\n    padding: 0px;\n    height: 1px;\n    display:flex;\n    justify-content:center;\n    align-items:center;\n}\n.calendar__table{\n    position: relative;\n    /* width: 276px; */\n    height: 178px;\n    bottom: 0;\n    margin: 92px 25px 16px 25px;\n    font-family: -apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif;\n    line-height: 16px;\n    font-size: 12px;\n    text-align: center;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    /* display: grid;\n    grid-template-columns: repeat(7, 1fr);\n    grid-column-gap: 25px;\n    grid-template-rows: repeat(5, 1fr);\n    grid-gap: 25px; */\n\n}\n.calendar__num{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    width: 32px;\n    height: 32px;\n    cursor: pointer;\n}\n.calendar__week{\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    margin: 5px 16px 5px 22px;\n    width: 298px;\n    font-family: -apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif;\n    line-height: 16px;\n    font-size: 12px;\n    color: #3F3356;\n    padding: 0px;\n}\n.calendar__num_opancy{\n    color: #ECEBED;\n}\n.calendar__num_sunday{\n    color: #FFA26B;\n}\n.calendar__num_current-day{\n    width: 32px;\n    height: 32px;\n    border-radius: 50%;\n    background:linear-gradient(225deg, #BD7AE3 0%, #8461C9 100%);\n    color: #FFFFFF;\n}\n.calendar__num_choose-day{\n    width: 32px;\n    height: 32px;\n    border-radius: 50%;\n    background-color:#EEDFF2;\n    color: #BE52F2;\n}"},"lineCount":null}},"error":null,"hash":"c5dab579bab1b0d3e476624160e993ee","cacheData":{"env":{}}}